@model IEnumerable<object>
@{
    ViewBag.Title = "Index";
}

<h2>Show All Services</h2>

@{ 
    List<DAL.Food> Foods = Model.ToList()[0] as List<DAL.Food>;
    List<DAL.Catering> Caterings = Model.ToList()[1] as List<DAL.Catering>;
    List<DAL.Decoration> Decorations = Model.ToList()[2] as List<DAL.Decoration>;
    List<DAL.Place> Places = Model.ToList()[3] as List<DAL.Place>;
    List<DAL.Photography> Photographies = Model.ToList()[4] as List<DAL.Photography>;
}
<h3>Foods</h3>
@if (Foods.Any())
{
    foreach (var item in Foods)
    {
        <li>@item.FoodTitle</li>
    }
}
else{ 
    <p style="color:red">Food menu added</p>
}
<h3>Caterings</h3>
@if (Caterings.Any())
{
    foreach (var item in Caterings)
    {
        <li>@item.CateringTitle</li>
    }
}
else
{
    <p style="color:red">No Catering service added</p>
}
<h3>Decorations</h3>
@if (Decorations.Any())
{
    foreach (var item in Decorations)
    {
        <li>@item.DecorationTitle</li>
    }
}
else
{
    <p style="color:red">No decorartion service added</p>
}
<h3>Photographies</h3>
@if (Photographies.Any())
{
    foreach (var item in Photographies)
    {
        <li>@item.PhotographyTitle</li>
    }
}
else
{
    <p style="color:red">No Photography service added</p>
}
<h3>Places</h3>
@if (Places.Any())
{
    foreach (var item in Places)
    {
        <li>@item.PlaceTitle</li>
    }
}
else
{
    <p style="color:red">no place added</p>
}